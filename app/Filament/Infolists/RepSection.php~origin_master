<?php

declare(strict_types=1);
/*
 * @see https://coderflex.com/blog/create-advanced-filters-with-filament
 */

namespace Modules\Ptv\Filament\Infolists;

use Filament\Infolists\Components;


class RepSection extends Components\Section {

    public static function getDefaultName(): ?string
    {
        return 'rep';
    }

    protected function setUp(): void
    {
       
        parent::setUp();

         $this->schema([
            Components\Grid::make(4)->schema([
                Components\TextEntry::make('stabi'),
                Components\TextEntry::make('stabi_txt'),
                Components\TextEntry::make('repar'),
                Components\TextEntry::make('repar_txt'),
            ]),
        ]);
    }


}