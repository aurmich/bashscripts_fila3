{
    "name": "laraxot/module_badge_fila3",
    "description": "",
    "license": "MIT",
    "homepage": "https://github.com/laraxot/module_badge_fila3.git",
    "authors": [
        {
            "name": "Marco Sottana",
            "email": "marco.sottana@gmail.com"
        }
    ],
    "require": {

    },
    "require-dev": {},
    "repositories": [
        {
            "type": "path",
            "url": "../Xot"
        },
        {
            "type": "path",
            "url": "../Tenant"
        },
        {
            "type": "path",
            "url": "../UI"
        }
    ],
    "autoload": {
        "psr-4": {
            "Modules\\Badge\\": "app/",
            "Modules\\Badge\\Database\\Factories\\": "database/factories/",
            "Modules\\Badge\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "scripts": {
        "post-autoload-dump": [],
        "post-update-cmd": [],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/pest --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\Badge\\Providers\\BadgeServiceProvider",
                "Modules\\Badge\\Providers\\Filament\\AdminPanelProvider"
            ],
            "aliases": {}
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
